{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "dist",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "lib": ["ES6","DOM"]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": []
}
